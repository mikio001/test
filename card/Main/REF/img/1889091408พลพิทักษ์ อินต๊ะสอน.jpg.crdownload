 <div id="box-login">
                    <script type="text/javascript">
               function login(){
                    var msg = "";
                    var error = false;
                    if(!$('#username').val())	{
                         $('#username').css({'border-color':'#F63','background-color':'#FFE1D7'});
                         msg = '**  กรุณากรอกชื่อผู้ใช้งาน\n';
                         error = true;
                    }
                    if (!$('#password').val())	{
                         $('#password').css({'border-color':'#F63','background-color':'#FFE1D7'});
                         msg = msg+'**  กรุณากรอกรหัสผ่าน\n';
                         error = true;
                    }
                    if(error)	{
                        // alert(msg);
                    } else {           
                        
                         //$('#status').html('');
                         $('#imgLogin').attr('src',base_url+'resources/images/loading.gif');
                         $.ajax({
                              type: "POST",
                              url: base_url+"auth/request_key",
                              success: function(data){
                                   var pass = $('#password').val();                                  
                                   var ad = Base64.encode(pass+'|'+data+'|'+$('#username').val());
                                   //                                   var ad = hex_md5(pass+data)+'|'+$('#username').val();                                  
                                   $.ajax({
                                        data:"authen="+ad,
                                        type: "POST",
                                        url: base_url+"auth/login",
                                        success: function(data){    
                                            console.log(data.trim().length);
                                            if (data.trim()=="1"){
                                                $.notify({
                                                        title: '<strong></strong>',
                                                        message: 'เข้าใช้งานสำเร็จ'
                                                },{
                                                        type: 'success'
                                                });
                                               $.ajax({ url: base_url+"auth/get_login", success: function(data){ $("#box-login").html(data) } }); 
                                            } else {
                                                
                                                 $.notify({
                                                        title: '<strong>!!!</strong>',
                                                        message: data
                                                },{
                                                        type: 'danger'
                                                });
                                            }
                                            
                                           //  $('#imgLogin').attr('src',base_url+'resources/themes01/front/img/login_bt.gif');
                                        }
                                   });
                              }
                         });
                    }
               };
          </script>
          <form id="login_form" class="login100-form validate-form" name="login_form" method="post" onsubmit="return false;">
              
              
              <div class="wrap-input100 validate-input m-b-16" data-validate = "Please Input Username">
						<input class="input100" type="text" name="username" id="username"  placeholder="Enter Username">
